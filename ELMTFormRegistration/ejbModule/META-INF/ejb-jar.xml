<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar version="2.1" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd">
    <display-name>FormRegistration</display-name>
    <enterprise-beans>
        <session>
            <display-name>KaverySessionBeanStatfulSB</display-name>
            <ejb-name>HLCKaverySessionBeanStatfulBean</ejb-name>
            <home>com.hlckavery.statful.HLCKaverySessionBeanStatfulRemoteHome</home>
            <remote>com.hlckavery.statful.HLCKaverySessionBeanStatfulRemote</remote>
            <ejb-class>com.hlckavery.statful.HLCKaverySessionBeanStatfulBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <entity>
            <display-name>ArabianSeaEntityEB</display-name>
            <ejb-name>HLCArabianSeaEntityBean</ejb-name>
            <local-home>com.hlcreg.form.HLCArabianSeaEntityLocalHome</local-home>
            <local>com.hlcreg.form.HLCArabianSeaEntityLocal</local>
            <ejb-class>com.hlcreg.form.HLCArabianSeaEntityBean</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.lang.String</prim-key-class>
            <reentrant>false</reentrant>
        </entity>
        </enterprise-beans>
    <assembly-descriptor>
        <container-transaction>
            <method>
                <ejb-name>HLCArabianSeaEntityBean</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
        <container-transaction>
            <method>
                <ejb-name>HLCKaverySessionBeanStatfulBean</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
        </assembly-descriptor>
    </ejb-jar>
