<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar version="2.1" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd">
    <display-name>SponserEJB</display-name>
    <enterprise-beans>
        <session>
            <display-name>SponsorSessionSB</display-name>
            <ejb-name>HLCSponsorSessionBean</ejb-name>
            <home>com.hlcspnr.sponsor.HLCSponsorSessionRemoteHome</home>
            <remote>com.hlcspnr.sponsor.HLCSponsorSessionRemote</remote>
            <ejb-class>com.hlcspnr.sponsor.HLCSponsorSessionBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <entity>
            <display-name>KrishnaRajaSaragEntityEB</display-name>
            <ejb-name>HLCKrishnaRajaSaragEntityBean</ejb-name>
            <local-home>com.hlcspnr.sponsor.HLCKrishnaRajaSaragEntityLocalHome</local-home>
            <local>com.hlcspnr.sponsor.HLCKrishnaRajaSaragEntityLocal</local>
            <ejb-class>com.hlcspnr.sponsor.HLCKrishnaRajaSaragEntityBean</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.lang.String</prim-key-class>
            <reentrant>false</reentrant>
        </entity>
        <entity>
            <display-name>SponsorPlanEB</display-name>
            <ejb-name>HLCSponsorPlanBean</ejb-name>
            <local-home>com.hlcspnr.plan.HLCSponsorPlanLocalHome</local-home>
            <local>com.hlcspnr.plan.HLCSponsorPlanLocal</local>
            <ejb-class>com.hlcspnr.plan.HLCSponsorPlanBean</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.lang.String</prim-key-class>
            <reentrant>false</reentrant>
        </entity>
        <entity>
            <display-name>SponsorPaymentScheduleEB</display-name>
            <ejb-name>HLCSponsorPaymentScheduleBean</ejb-name>
            <local-home>com.hlcspnr.schedule.HLCSponsorPaymentScheduleLocalHome</local-home>
            <local>com.hlcspnr.schedule.HLCSponsorPaymentScheduleLocal</local>
            <ejb-class>com.hlcspnr.schedule.HLCSponsorPaymentScheduleBean</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.lang.String</prim-key-class>
            <reentrant>false</reentrant>
        </entity>
        </enterprise-beans>
    <assembly-descriptor>
        <container-transaction>
            <method>
                <ejb-name>HLCSponsorSessionBean</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
        <container-transaction>
            <method>
                <ejb-name>HLCKrishnaRajaSaragEntityBean</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
        <container-transaction>
            <method>
                <ejb-name>HLCSponsorPlanBean</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
        <container-transaction>
            <method>
                <ejb-name>HLCSponsorPaymentScheduleBean</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
        </assembly-descriptor>
    </ejb-jar>
