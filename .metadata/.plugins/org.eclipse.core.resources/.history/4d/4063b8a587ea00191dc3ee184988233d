
package com.hlcspnr.schedule;

import javax.ejb.EJBLocalHome;
import javax.ejb.FinderException;
import java.util.*;
import javax.ejb.*;


/**
 * This is the local-home interface for SponsorPaymentSchedule enterprise bean.
 */

public interface HLCSponsorPaymentScheduleLocalHome extends EJBLocalHome {
    public HLCSponsorPaymentScheduleLocal create(String sponsorId,String scheduleTypeId,
            Date dueDate,float dueAmount , boolean continueStatus,
            String frequencyRate, String paymentStatus, Vector specialBil)  throws CreateException;
    
    public HLCSponsorPaymentScheduleLocal findByPrimaryKey(String scheduleId)  throws FinderException;

    //public Collection findByScheduleId(String scheduleId) throws FinderException;
    public Collection findByAll() throws FinderException;
    public Collection findBySponsorId(String sponsorId) throws FinderException;
    //public Collection findBySponsorTypeId(String sponsorId) throws FinderException;
}

