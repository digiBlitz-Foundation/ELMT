function myvalidate(){

//-------------------------------------Event Type name------------------------------------------------------------

if((document.frmMeeEventTypeAdd.eventName.value=="")||(document.frmMeeEventTypeAdd.eventName.value.indexOf(' ')==0))
   {alert(" Event Type name cannot be empty ");
     document.frmMeeEventTypeAdd.eventName.focus();
    return false; }
	
	if((document.frmMeeEventTypeAdd.eventName.value.length >80 ))
	
 {alert(" Event Type name exceeds the maximum of 80 characters");
  document.frmMeeEventTypeAdd.eventName.focus();
 return false;}
 
 if(document.frmMeeEventTypeAdd.eventName.value.indexOf('  ')!==-1){
		alert("Enter Valid Event Name.");
		document.frmMeeEventTypeAdd.eventName.focus();
		return false;
	}
	
	
    return true;
    }
	